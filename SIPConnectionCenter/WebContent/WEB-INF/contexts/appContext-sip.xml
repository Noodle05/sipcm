<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.0.xsd">
	<bean id="voipVendorManager" class="com.sipcm.sip.vendor.VoipVendorManager">
		<lookup-method name="createSipVoipVendorContext" bean="sipVoipVendorContext" />
	</bean>

	<bean id="googleVoiceManager" class="com.sipcm.googlevoice.GoogleVoiceManager">
		<lookup-method name="getGoogleVoiceSession" bean="googleVoiceSession" />
	</bean>

	<util:map id="voipVendorToServletMap" key-type="com.sipcm.sip.VoipVendorType"
		value-type="java.lang.String">
		<entry key="SIP" value="DelegatedServlet" />
		<entry key="GOOGLE_VOICE" value="GoogleVoiceServlet" />
	</util:map>

	<util:list id="sip.registrationEventListeners">
		<ref bean="userSipProfileOnlineStatusUpdater" />
	</util:list>

	<bean id="userServiceEventListener" class="com.sipcm.base.CompositeServiceEventListener">
		<property name="listeners">
			<util:list>
				<ref bean="locationServiceUserEventListener" />
				<ref bean="vendorManagerUserEventListener" />
			</util:list>
		</property>
	</bean>
</beans>